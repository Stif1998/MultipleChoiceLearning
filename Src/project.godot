[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="MultipleChoiceLearning"

[rendering]

renderer/rendering_method="mobile"
environment/defaults/default_clear_color=Color(0.691357, 0.691357, 0.691357, 1)
